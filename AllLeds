import sys
from naoqi import ALProxy
IP = "192.168.1.1"
PORT = 9559

try:
    proxy = ALProxy("ALLeds", IP, PORT)
except Exception,e:
    print "Could not create proxy to ALLeds"
    print "Error was: ",e
    sys.exit(1)

# Example showing how to switch on a group
name = 'FaceLeds'
proxy.on(name)

sys.exit(0)
